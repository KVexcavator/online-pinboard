<% if @search %>
  <% @search.each do |s| %>
    <% if s.kind_of?(Board) %>
      <li>
        <p>
          <%= s.title %>
          <br/>
          <%= s.description %>
          <br/>
          <%= s.user.email %>
        </p>
      </li>
    <% elsif s.class.name == "Pin" %>
      <li>
        <%=image_tag s.image.url%>
        <p>
          <%= s.board.title%>
        </p>
      </li>
    <% end %>
  <%end%>
<% end %>